extends ../layout.pug

block prepend styles
  link(rel='stylesheet' href='//cdn.datatables.net/1.10.19/css/jquery.dataTables.min.css')

block content
  div(class='breadcrumb')
    a(href='/ticket/add') Ticket Add  
    a(href='/ticket/edit') Ticket Edit  
    a(href='/ticket/remove') Ticket Remove  
  
  form(id='ticket-remove-form' name='ticket-remove-form' class='ticket-inline' action='')
    div(class="form-group")
      div(class="col-md-3")
        label(for="id") Ticket id
      div(class="col-md-9")
        input(id="id" name="id")
    div(class="form-group")
      div(class="col-md-12")
       button(type='submit' class='btn btn-danger') Remove
block append scripts
  script(src='/static/js/models/ticket.js')
  script(src='/static/js/models/ticketSaled.js')
  script(src='//cdn.datatables.net/1.10.19/js/jquery.dataTables.min.js')
  script(src='/static/js/views/ticketSaled.js')
      
      